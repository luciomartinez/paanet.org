<section class="view-content container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3 text-center">
      <h1>Proyectos</h1>
      <% @projects.each do |project| %>
        <div>
          <%= link_to project.title, project.url %>
          <p><%= project.description %></p>
        </div>
      <% end %>
    </div>
  </div>
</section>
